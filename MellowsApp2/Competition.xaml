<UserControl x:Class="MellowsApp2.Competition"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MellowsApp2"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="1366">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition Height="350"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="170"/>
        </Grid.RowDefinitions>
        <Border Grid.Row="0" Grid.RowSpan="4" Width="1366" BorderThickness="5">
            <Rectangle Grid.Row="2" RadiusX="15" RadiusY="15" Margin="0,30,0,-30">
                <Rectangle.Fill>
                    <LinearGradientBrush>
                        <GradientStop Color="DarkGreen" Offset="0.0"/>
                        <GradientStop Color="LightGreen" Offset="0.25"/>
                        <GradientStop Color="DarkGreen" Offset="0.5"/>
                        <GradientStop Color="LightGreen" Offset="0.75"/>
                        <GradientStop Color="DarkGreen" Offset="1.0"/>
                    </LinearGradientBrush>
                </Rectangle.Fill>
            </Rectangle>
        </Border>
        <Label x:Name="competitionName" Content="Competition Name" Grid.Row="0" FontSize="22" 
               FontWeight="Bold" HorizontalAlignment="Center"></Label>
        <Button x:Name="returnToMembers" Content="Members" Width="150" Height="30" VerticalAlignment="Bottom" 
                FontSize="14" FontWeight="Bold" Style="{StaticResource ButtonRound}" 
                Click="ReturnToMembersClick" Background="LightBlue"></Button>

        <DataGrid x:Name="competition" Grid.Row="1" AutoGenerateColumns="False" Height="300" Width="975" 
          Background="Transparent" CanUserAddRows="False" SelectionUnit="Cell" 
          VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" Visibility="Visible"
                  SelectedCellsChanged="Competition_SelectedCellsChanged" Margin="196,35,195,15">
            <DataGrid.Columns>
                <DataGridTextColumn x:Name="Id2" Header="Id" Width="30" CanUserResize="False" 
                                    Visibility="Hidden" Binding="{Binding Path=Id}"/>
                <DataGridTextColumn x:Name="Member2" Header="Player Name" Width="150" CanUserResize="False" 
                                    CanUserReorder="False" Binding="{Binding Path=Member}" IsReadOnly="True"/>
                <DataGridTextColumn x:Name="RegNo" Header="Reg No" Width="60" CanUserResize="False" 
                                    CanUserReorder="False" Binding="{Binding Path=RegNo}" IsReadOnly="True"/>
                <DataGridTemplateColumn Header="" x:Name="addPlayerScore" Width="40" IsReadOnly="True" 
                                        DisplayIndex="3" Visibility="Visible">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Image Source="Images/plus.png" ToolTip="Add Player Score" Width="20" Height="20"/>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTemplateColumn Header="" x:Name="hidePlayer" Width="40" IsReadOnly="True" 
                        DisplayIndex="4" Visibility="Visible">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Image Source="Images/minus.png" ToolTip="Hide Player" Width="20" Height="20"/>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTextColumn x:Name="G18_1" Header="18G(1)" Width="45" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Gross1}"/>
                <DataGridTextColumn x:Name="G18_2" Header="18G(2)" Width="45" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Gross2}"/>
                <DataGridTextColumn x:Name="N18_1" Header="18N(1)" Width="45" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Nett1}"/>
                <DataGridTextColumn x:Name="N18_2" Header="18N(2)" Width="45" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Nett2}"/>
                <DataGridTextColumn x:Name="G36" Header="36-G" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Gross36}"/>
                <DataGridTextColumn x:Name="N36" Header="36-N" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Nett36}"/>
                <DataGridTextColumn x:Name="B9G" Header="B9-G" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Back9Score}"/>
                <DataGridTextColumn x:Name="B6G" Header="B6-G" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Back6Score}"/>
                <DataGridTextColumn x:Name="B3G" Header="B3-G" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Back3Score}"/>
                <DataGridTextColumn x:Name="B2G" Header="B2-G" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Back2Score}"/>
                <DataGridTextColumn x:Name="B1G" Header="B1-G" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Back1Score}"/>
                <DataGridTextColumn x:Name="B9N" Header="B9-N" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Back9ScoreNett}"/>
                <DataGridTextColumn x:Name="B6N" Header="B6-N" Width="40" CanUserResize="False" 
                                    Visibility="Visible" Binding="{Binding Path=Back6ScoreNett}"/>
                <DataGridTextColumn x:Name="B3N" Header="B3-N" Width="40" CanUserResize="False" 
